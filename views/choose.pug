extends layout

block content
  h1= title
  - let image_names = ["Apart1.jpg","Apart2.jpg","Apart3.jpg","Apart5.jpg","Apart4.jpg"]
  - for (let i = 0; i < 3; i++)
    - let random = Math.floor(Math.random() * 5)
    img(src=`/images/${image_names[random]}`, alt=`Image Name Apartment ${random}`, height="300px", width="300px")
